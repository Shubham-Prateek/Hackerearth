#include <stdio.h>

int main()
{
	int t;
	scanf("%d", &t);
	for(int i=0;i<t;i++)
	{
		int x,y,a,b,l,m;
		scanf("%d %d %d %d %d %d",&x,&y,&l,&m,&a,&b);
		{
			if(l+a<=x)
			{
				if(b-m>=0)
					printf("bottom-right\n");
				else
					printf("top-right\n");
			}
			else
			{
				if(b-m>=0)
					printf("bottom-left\n");
				else
					printf("top-left\n");
			}
		}
	}
}
