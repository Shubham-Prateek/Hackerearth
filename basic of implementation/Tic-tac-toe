#include <stdio.h>

int main()
{
	char a[3],b[3],c[3];
	int x=0,o=0,i;
	scanf("%s",a);
	scanf("%s %s",b,c);
	for( i=0;i<3;i++)
	{
		if(a[i]=='X')
			x=x+1;
		else if(a[i]=='O')
			o=o+1;
		if(b[i]=='X')
			x=x+1;
		else if(b[i]=='O')
			o=o+1;
		if(c[i]=='X')
			x=x+1;
		else if(c[i]=='O')
			o=o+1;
	}
	if(o>x)
	{
		printf("Wait, what?");
	}
	else if(x>(o+1))
	{
		printf("Wait, what?");
	}
	else if(x==o)
	{
		if((a[0]==a[1]&&a[0]==a[2]&&a[0]=='X')||(b[0]==b[1]&&b[0]==b[2]&&b[0]=='X')||(c[0]==c[1]&&c[0]==c[2]&&c[0]=='X')||(a[0]==b[0]&&a[0]==c[0]&&a[0]=='X')||(a[1]==b[1]&&a[1]==c[1]&&a[1]=='X')||(a[2]==b[2]&&a[2]==c[2]&&a[2]=='X')||(a[0]==b[1]&&a[0]==c[2]&&a[0]=='X')||(a[2]==b[1]&&a[2]==c[0]&&a[2]=='X'))
		printf("Wait, what?");
		else if((a[0]==a[1]&&a[0]==a[2]&&a[0]=='O')||(b[0]==b[1]&&b[0]==b[2]&&b[0]=='O')||(c[0]==c[1]&&c[0]==c[2]&&c[0]=='O')||(a[0]==b[0]&&a[0]==c[0]&&a[0]=='O')||(a[1]==b[1]&&a[1]==c[1]&&a[1]=='O')||(a[2]==b[2]&&a[2]==c[2]&&a[2]=='O')||(a[0]==b[1]&&a[0]==c[2]&&a[0]=='O')||(a[2]==b[1]&&a[2]==c[0]&&a[2]=='O'))
		printf("O won.");
		else
		printf("X's turn.");
		
	}
	else if((x!=o)&&(x+o)!=9)
	{
		if((a[0]==a[1]&&a[0]==a[2]&&a[0]=='X')||(b[0]==b[1]&&b[0]==b[2]&&b[0]=='X')||(c[0]==c[1]&&c[0]==c[2]&&c[0]=='X')||(a[0]==b[0]&&a[0]==c[0]&&a[0]=='X')||(a[1]==b[1]&&a[1]==c[1]&&a[1]=='X')||(a[2]==b[2]&&a[2]==c[2]&&a[2]=='X')||(a[0]==b[1]&&a[0]==c[2]&&a[0]=='X')||(a[2]==b[1]&&a[2]==c[0]&&a[2]=='X'))
		printf("X won.");
		else if((a[0]==a[1]&&a[0]==a[2]&&a[0]=='O')||(b[0]==b[1]&&b[0]==b[2]&&b[0]=='O')||(c[0]==c[1]&&c[0]==c[2]&&c[0]=='O')||(a[0]==b[0]&&a[0]==c[0]&&a[0]=='O')||(a[1]==b[1]&&a[1]==c[1]&&a[1]=='O')||(a[2]==b[2]&&a[2]==c[2]&&a[2]=='O')||(a[0]==b[1]&&a[0]==c[2]&&a[0]=='O')||(a[2]==b[1]&&a[2]==c[0]&&a[2]=='O'))
		printf("Wait, what?");
		else
		printf("O's turn.");
	}
	else
	{
		if((a[0]==a[1]&&a[0]==a[2]&&a[0]=='X')||(b[0]==b[1]&&b[0]==b[2]&&b[0]=='X')||(c[0]==c[1]&&c[0]==c[2]&&c[0]=='X')||(a[0]==b[0]&&a[0]==c[0]&&a[0]=='X')||(a[1]==b[1]&&a[1]==c[1]&&a[1]=='X')||(a[2]==b[2]&&a[2]==c[2]&&a[2]=='X')||(a[0]==b[1]&&a[0]==c[2]&&a[0]=='X')||(a[2]==b[1]&&a[2]==c[0]&&a[2]=='X'))
		printf("X won.");
		else if((a[0]==a[1]&&a[0]==a[2]&&a[0]=='O')||(b[0]==b[1]&&b[0]==b[2]&&b[0]=='O')||(c[0]==c[1]&&c[0]==c[2]&&c[0]=='O')||(a[0]==b[0]&&a[0]==c[0]&&a[0]=='O')||(a[1]==b[1]&&a[1]==c[1]&&a[1]=='O')||(a[2]==b[2]&&a[2]==c[2]&&a[2]=='O')||(a[0]==b[1]&&a[0]==c[2]&&a[0]=='O')||(a[2]==b[1]&&a[2]==c[0]&&a[2]=='O'))
		printf("Wait, what?");
		else
		printf("It's a draw.");
	}


}
