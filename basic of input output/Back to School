#include <stdio.h>

int main()
{
	int n,a,b,c;
	scanf("%d %d %d", &a,&b,&c);
	if(a>b&&a>c)
	{
		//if(b>c)
		printf("%d",a);
		//else if(b<c)
		//printf("%d %d %d",a,c,b);
	} 
	if(c>b&&c>a)
	{
		//if(b>a)
		printf("%d",c);
		//else if(b<a)
		//printf("%d %d %d",c,a,b);
	} 
	if(b>a&&b>c)
	{
		//if(a>c)
		//printf("%d %d %d",b,a,c);
		//else if(a<c)
		printf("%d",b);
	} 
	return 0;
}
