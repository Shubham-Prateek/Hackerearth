#include <stdio.h>
int main()
{
	int num,k=0,m=0,n=0,p=0,i=1;
	scanf("%d", &num);  
	while(num>0)
	{
		if(num>i)
			{
				p=i;
				num=num-i;
			}
		else
		{
			printf("Patlu");
			break;
		}
		if(num>(i*2))
			{
				m=i*2;
				num=num-(i*2);
			}
		else
		{
			printf("Motu");
			break;
		}
		i++;

	}
}
