#include <stdio.h>

int main()
{
	int num;
	scanf("%d", &num);
	for(int i=0;i<num;i++)
	{
		int n;
		scanf("%d",&n);				
		if((n%12)==0)
			printf("%d WS\n",n-11);
		else if((n-1)%12==0)
			printf("%d WS\n",n+11);
		else if((n-2)%12==0)
			printf("%d MS\n",n+9);
		else if((n-3)%12==0)
			printf("%d AS\n",n+7);
		else if((n-4)%12==0)
			printf("%d AS\n",n+5);
		else if((n-5)%12==0)
			printf("%d MS\n",n+3);
		else if((n-6)%12==0)
			printf("%d WS\n",n+1);
		else if((n-7)%12==0)
			printf("%d WS\n",n-1);
		else if((n-8)%12==0)
			printf("%d MS\n",n-3);
		else if((n-9)%12==0)
			printf("%d AS\n",n-5);
		else if((n-10)%12==0)
			printf("%d AS\n",n-7);
		else if((n-11)%12==0)
			printf("%d MS\n",n-9);	
	}
	return 0;
}
