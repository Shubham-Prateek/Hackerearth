#include <stdio.h>

int main()
{
	int num,n;
	scanf("%d", &num);
	for(int i=0;i<num;i++)
	{
		scanf("%d",&n);
		for(int j=0;j<n;j++)
		{
			for(int k=0;k<=j;k++)
			{
				printf("*");
			}
			for(int k=j+1;k<n;k++)
			{
				printf("#");
			}

			for(int k=n+1;k<=2*n-j-1;k++)
			{
				printf("#");
			}
			for(int k=2*n-j+1;k<=2*n+1;k++)
			{
				printf("*");
			}




			printf("\n");
		}
	}
}
